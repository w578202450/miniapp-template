.flexRowStartCenter {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}

.middle-one {
  height: 140rpx;
  background: #fff;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  left: 0;
  right: 0;
  z-index: 3;
  background: url('https://com-shuibei-peach-static.oss-cn-shenzhen.aliyuncs.com/tmcpro/images/home/inquiry_bg.png');
  background-size: 100% 100%;
  background-color: rgb(162, 170, 187);
}

.middleOne-box {
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding-left: 20rpx;
}

.middleOne-box-one {
  border-right: 1rpx solid #dbdee3;
}

.headDiv {
  height: 80rpx;
  width: 80rpx;
  border-radius: 80rpx;
  background-color: rgb(248, 248, 248);
  overflow: hidden;
  position: relative;
}

.middleOne-box .headImg {
  height: 80rpx;
  width: 80rpx;
  position: relative;
  z-index: 2;
}

.headImgBac {
  height: 80rpx;
  width: 80rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}

.middleOne-box .info-box {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}

.middleOne-box .info-box .info_one {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  color: #ffffff;
  max-width: 150rpx;
}

.middleOne-box .info-box .info_one_one {
  margin-left: 10rpx;
  border-radius: 30rpx;
  border: 1px solid #438bef;
  padding: 0 15rpx;
  font-size: 28rpx;
  color: #438bef;
}

.middleOne-box .info-box .info_two {
  margin-top: 16rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 28rpx;
}

.middle-one .lineView {
  width: 1rpx;
  height: 80rpx;
  background: rgba(232, 232, 232, 1);
}

/*聊天记录*/

.message-list {
  height: 100vh;
  background-color: #f6f6f6;
}

.message-list-two {
  height: calc(100vh - 242rpx);
  background-color: #f6f6f6;
}

/*空置盒子1*/

.marginBoxOne {
  height: 260rpx;
}

.marginBoxOne1 {
  height: 310rpx;
}

.marginBoxOne2 {
  height: 250rpx;
}
.marginBoxOne3{
height:150rpx;
}

/*空置盒子2*/

.marginBoxTwo {
  height: 120rpx;
}

.marginBoxTwoSecond {
  height: 362rpx;
}

/* 结束问诊提示盒子 */

.endTMCInqueryDiv {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

/* 结束问诊提示文字 */

.endTMCInqueryTxt {
  font-size: 26rpx;
  color: #818181;
  border-radius: 14rpx;
  padding: 8rpx 20rpx;
}

/*单元行*/

.row {
  display: flex;
  flex-direction: column;
  margin: 0 30rpx;
}

/*日期*/

.datetime {
  font-size: 28rpx;
  padding: 20rpx 0 0 0;
  color: #999;
  text-align: center;
}

/*主体*/

.body {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
  width: 100%;
  margin-top: 10px;
}

/*头像容器*/

.body.avatar-container {}

.body .personHeadImgDiv {
  position: relative;
}

/* 患者头像时 */

.body .personHeadImgBac {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #fff;
  position: absolute;
  left: 20rpx;
  right: 0;
}

/* 医生、医助头像时 */

.body .personHeadImgBacTwo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #fff;
  position: absolute;
  left: 0;
  right: 20rpx;
}

/*头像*/

.body .avatarOne {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #fff;
  margin-right: 20rpx;
}

.body .avatarTwo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #fff;
  margin-left: 20rpx;
}

/*人名*/

.personNameTxt {
  font-size: 30rpx;
  color: #86888b;
}

/*文本消息*/

.body .contentTxtMsg {
  font-size: 32rpx;
  background: #fff;
  border-radius: 15rpx;
  padding: 15rpx;
  margin-bottom: 20rpx;
}

/*图片消息*/

.body .contentPictureMsg {
  background: #fff;
  margin-bottom: 20rpx;
  width: 240rpx;
  position: relative;
  overflow: hidden;
}

.body .imageLoading {
  position: absolute;
  left: -60rpx;
  width: 50rpx;
  height: 50rpx;
}

.body .pictureMsg {
  width: 240rpx;
  height: 240rpx;
}

/*音频消息*/

.body .contentVideoMsg {
  margin-bottom: 20rpx;
  padding: 15rpx;
  border-radius: 15rpx;
  background: #fff;
}

.body .recordDivSelf {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
  flex-wrap: wrap;
}

.body .recordDiv {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}

.body .payloadImage {
  height: 20px;
  width: 14px;
  margin-left: 15rpx;
  margin-right: 15rpx;
}

/*自定义卡片消息*/

.cardRpBox {
  background: #fff;
  border-radius: 15rpx;
  background-color: #6aca6b;
  margin-bottom: 20rpx;
  width: 450rpx;
}

.cardRpBox .cardRpTitleDiv {
  padding: 20rpx 15rpx;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.cardRpBox .cardRpIcon {
  width: 55rpx;
  height: 55rpx;
  margin-right: 15rpx;
  flex-shrink: 0;
}

.cardRpBox .cardRpInfo {
  font-size: 30rpx;
  color: #fff;
  width: 400rpx;
}

.cardRpBox .toRpDetailDiv {
  background-color: #fff;
  padding: 15rpx 15rpx;
  border-bottom-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.cardRpBox .toRpDetailTitle {
  color: #555658;
  font-size: 30rpx;
}

.cardRpBox .toRpDetailIcon {
  width: 15rpx;
  height: 27rpx;
}

/* 自定义素材消息：图文*/

.materialCard {
  width: 550rpx;
  background: #fff;
  border-radius: 15rpx;
  padding: 15rpx;
  margin-bottom: 20rpx;
}

.materialCard .materialTitle {
  font-size: 32rpx;
  line-height: 38rpx;
  color: #242526;
  word-break: break-all;
}

.materialCard .materialInfo {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
  margin-top: 15rpx;
}

.materialCard .materialInfo .materialDescribe {
  width: 73%;
  font-size: 30rpx;
  line-height: 36rpx;
  color: #555658;
  word-break: break-all;
}

.materialCard .materialInfo .materialImgBox {
  position: relative;
  width: 120rpx;
  height: 120rpx;
}

.materialCard .materialInfo .materialImg {
  width: 100%;
  height: 100%;
  background-color: rgb(245, 245, 245);
}

.materialCard .materialInfo .videoPlayIcon {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 40rpx;
  height: 40rpx;
}

.materialImageAll {
  width: 300rpx;
  height: 200rpx;
}

/* 自定义视频聊天消息 */

.videoMsgBox {
  margin-bottom: 20rpx;
  padding: 15rpx;
  border-radius: 15rpx;
  background: #fff;
}

.videoDiv {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.videoMsgBox .videoMsgIcon {
  width: 40rpx;
  height: 40rpx;
}

.videoMsgBox .videoStatusInfo {
  font-size: 32rpx;
  margin-right: 15rpx;
}

.videoMsgBox .videoStatusInfoDoc {
  font-size: 32rpx;
  margin-left: 15rpx;
}

/*底部工具栏*/

.bottomMenus {
  width: 100%;
  position: fixed;
  bottom: 0;
  z-index: 10;
  background: #f6f6f6;
}

/*回复框*/

.reply {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}

/*文本输入或语音录入*/

.reply .opration-area {
  margin: 0 0 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

/*语音图标*/

.reply .opration-area .voiceIcon {
  width: 55rpx;
  height: 55rpx;
}

/*语音长按按钮*/

.reply .opration-area .recordBtn {
  margin: 20rpx 20rpx;
  width: 515rpx;
  padding: 0rpx 25rpx 0 15rpx;
  border-radius: 6px;
  height: 75rpx;
  background: #f6f6f6;
  text-align: center;
  line-height: 75rpx;
  color: #555658;
}

/*回复文本框*/

.reply .opration-area .msgInput {
  margin: 20rpx 20rpx;
  /* width: 445rpx; */
  /* width: 400rpx; */
  padding: 0rpx 25rpx 0 15rpx !important;
  border: none;
  border-radius: 6px;
  height: 75rpx;
  background: #f6f6f6;
}

/*恢复文本框图标*/

.reply .opration-area .inputIcon {
  width: 55rpx;
  height: 55rpx;
}

/*发送*/

.reply .send {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 20rpx;
  font-size: 34rpx;
  color: #999;
  text-align: center;
}

/*菜单按钮*/

.reply .menusIcon {
  width: 55rpx;
  height: 55rpx;
}

.reply .send .sendContentMsgBtn {
  background-color: #438bef;
  border-radius: 5rpx;
  padding: 4rpx 22rpx;
  color: #fff;
}

/*工具箱*/

.toolbarBox {
  width: 100%;
  padding: 20rpx 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
}

/*单个功能*/

.toolbarBox .toolbarDiv {
  margin: 0 20rpx 20rpx 0;
}

/*一行末尾的单个功能*/

.toolbarBox .toolbarDivFifth {
  margin: 0 0 20rpx 0;
}

/*功能按钮图标*/

.toolbarBox .toolbarIcon {
  border-radius: 10rpx;
  width: 126rpx;
  height: 126rpx;
  background: #fff;
}

/*功能按钮标题*/

.toolbarBox .toolbarTitile {
  width: 126rpx;
  text-align: center;
  font-size: 30rpx;
  color: #999;
}

/*选取图片*/

.choose-image {
  width: 25px;
  height: 25px;
  margin-right: 3%;
}

/*按住说话button*/

.voice-button {
  height: 36px;
  color: #818181;
  font-size: 14px;
  line-height: 36px;
}

/*悬浮提示框*/

.hud-container {
  position: fixed;
  width: 150px;
  height: 150px;
  left: 50%;
  top: 50%;
  margin-left: -75px;
  margin-top: -75px;
}

/*背景层*/

.hud-background {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #999;
  opacity: 0.8;
  z-index: 11;
  border-radius: 10px;
}

/*悬浮框主体*/

.hud-body {
  position: relative;
  width: 100%;
  height: 100%;
  z-index: 19;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

/*图标*/

.hud-body image {
  margin-top: 20px;
  width: 80px;
  height: 80px;
}

/*文字*/

.hud-body .tip {
  color: #fff;
  text-align: center;
  width: 90%;
  line-height: 34px;
  margin: 0 auto;
  margin-bottom: 10px;
}

.hud-body .warning {
  background: #c33;
  border-radius: 5px;
}

