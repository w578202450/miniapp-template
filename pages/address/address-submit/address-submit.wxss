.weui-cells{
  margin-top: 40rpx;
  width: 100%;
}

.weui-cell__bd{
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,0.85);
  line-height:32rpx;
  text-align: left;
}

.weui-cell__ft{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,0.45);
  line-height:28rpx;
}

.textarea{
  background-color: white;
}

.weui-textarea{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,0.45);
  line-height:28rpx;
}

.page button{
  margin-top: 30rpx;
  height:80rpx;
  background:linear-gradient(270deg,rgba(59,140,237,1) 0%,rgba(93,180,242,1) 100%);
  font-size:32rpx;
  font-family:PpackageingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  margin-left: 56rpx;
  width: 100%;
  border-radius:40rpx;
  border-style: solid;
  border-width: 0rpx;
  box-shadow:0px 2px 4px 0px rgba(93,180,242,1);
  -webkit-box-shadow:0px 2px 4px 0px rgba(93,180,242,1);
}

.container{
  background-color: white;
}