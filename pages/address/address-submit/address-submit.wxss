.header{
 position: absolute;
 width: 100%;
 height:386rpx;
 background-color: orange;
}

.paySuccess{
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 48rpx;
  width: 100%;
}

.paySuccess image{
  width:58rpx;
  height:58rpx;
}

.paySuccess text{
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:32rpx;
  margin-top: 34rpx;
}
.header-bgImage{
 width: 100%;
 height:386rpx;
}

.navbar{
  position: relative;
}
.back-icon{
  width:20rpx;
  height:36rpx;  
  position: absolute;    
  transform: translateY(-50%);    
  top: 50%;    
  display: flex;
}
.back-icon{    
  left: 16px;
}
.back-icon image{    
    width:20rpx;
    height:36rpx;   
    margin: auto;
}
.nav-title{    
    position: absolute;    
    transform: translate(-50%, -50%);    
    left: 50%;    
    top: 50%;    
    font-size:36rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
    line-height:50rpx;
}

