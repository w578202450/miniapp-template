<view class="page" style="{{pageStyle}}" bindtap="clickItem">
  <!-- 引入wxs脚本 -->
  <wxs src="../../../../wxs/wxsCommon.wxs" module="comTools" />
  <image style="border-radius: 10rpx;" src="{{pageItem.doctorDTOForTMC.photoUrl}}"></image>
  <view class="midlle-box">
    <text class="name-text">{{pageItem.doctorDTOForTMC.doctorName}}</text>
    <text class="other-text" style="margin-left:16rpx;line-height: 34rpx">{{pageItem.doctorDTOForTMC.duty}}</text>
  </view>
  <text class="other-text" style="margin-top:16rpx">{{pageItem.doctorDTOForTMC.workPlace}}</text>
  <text class="other-text">{{pageItem.doctorDTOForTMC.sectionName}}</text>
  <service-tabs list="{{comTools.strSplit('ssss,ffffrrrrrrrrrrrrrrrrrrrrrr,fff,ereer',',')}}" pageStyle="margin-top: 40rpx;margin-left: 40rpx;flex-wrap: nowrap;" tabStyle="background: rgba(67, 139, 239, 0.15);color:rgba(46,127,241,1);border:0rpx;"></service-tabs>

</view>