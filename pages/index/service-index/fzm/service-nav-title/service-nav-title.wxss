

.title-box{
  background: rgba(255, 255, 255, 1);
  position: fixed;
  width: 100%;
  z-index: 10
}

.title-default-box {
  display: flex;
  align-items: center;
  justify-content: center
}

.title-custom-box {
  display: flex;
  align-items: center;
  margin-left: 20rpx
}

.button-box {
  width: 160rpx;
  height: 48rpx;
  background: rgba(254, 83, 97, 0.99);
  box-shadow: 0px 2rpx 32rpx 0px rgba(0, 0, 0, 0.15);
  border-radius: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 20rpx;
}

.button-text {
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.banner-box {
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.title-name {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 36rpx;
}

.title-other {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.65);
  line-height: 24rpx;
}
