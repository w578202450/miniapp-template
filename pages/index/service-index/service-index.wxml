<!--pages/index/service-index/service-index.wxml-->
<view>
  <!-- 标题 -->
  <service-nav-title isInquiryNav="{{scrollTop == 0 ? false : true}}" doctorInfo="{{doctorInfo}}"></service-nav-title>
  <!-- 专家门诊专家信息 -->
  <service-specialistInfo pageStyle="margin-bottom:20rpx" doctorInfo="{{doctorInfo}}" doctorDisease="{{doctorDisease}}" certifyInfo="{{certifyInfo}}" navTitle="专家门诊" bind:previewDoctorInfo="previewDoctorInfo" bind:toOnlineInqueryFun="toOnlineInqueryFun"></service-specialistInfo>
  <!-- 医生团队 -->
  <service-physicianTeam pageStyle="margin-bottom:20rpx" doctorInfo="{{assistantDoctorInfo}}" doctorDisease="{{doctorDisease}}"></service-physicianTeam>
  <!-- 患者评价 -->
  <patient-evaluate wx:if="{{evaluateAllData.orgID && evaluateAllData.orgID.length > 0}}" evaluateAllData="{{evaluateAllData}}" />
  <!-- 医生手记 -->
  <notes paersonInfo="{{inquiryCaseData}}"></notes>
  <!-- 患者分享 -->
  <share paersonInfo="{{patientShareGetData}}"></share>
  <!-- 文章模块 -->
  <service-news pageStyle="margin-top:20rpx;margin-bottom:20rpx" articleTitles="{{articleTitles}}" articleDatas="{{articleDatas}}" currentCategoryData="{{currentCategoryData}}"></service-news>
</view>