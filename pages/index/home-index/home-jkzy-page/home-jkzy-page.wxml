<view class="page">
  <!-- 大冢制药banner -->

  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{background}}" wx:index="{{index}}" wx:key="bannerList">
      <swiper-item class="swiper-item">
        <image src="{{item}}" class="slide-image {{swiperIndex == index ? 'active' : ''}}" />
      </swiper-item>
    </block>
  </swiper>
  <!-- <view class="banner-header" style="width: 100%;">
    <image mode="widthFix" style="width: 100%;background-size: contain;" src="{{ homeBannerDefaultUrl }}"></image>
  </view> -->
  <view class="home">
    <view class="satisfy-box">
      <image class="image-box1" mode="widthFix" src="{{middleOnefei1}}"></image>
       <view class="center-doctor">
        <image src="https://com-shuibei-peach-static.oss-cn-shenzhen.aliyuncs.com/tmcpro/images/home/doctor-1.png"
          bindtap="toServiceIndexFun">
        </image>
        <image src="https://com-shuibei-peach-static.oss-cn-shenzhen.aliyuncs.com/tmcpro/images/home/doctor-2.png"
          bindtap="toServiceIndexFun">
        </image>
      </view>
     <view class="center-doctor_bt">
        <image src="https://com-shuibei-peach-static.oss-cn-shenzhen.aliyuncs.com/tmcpro/images/home/reg-1.png"
          bindtap="toServiceIndexFun"></image>
      </view>
    </view>

    <view class="bottom-img_container">
      <image class="rectangle-background-right1" mode="aspectFill" src="{{middleOnefei3}}" bindtap="goDetail"></image>
      <image class="rectangle-background-right2" mode="aspectFill" src="{{middleOnefei4}}" bindtap="goDetail2"></image>
    </view>
    <view class="enter-container">
      <view class="enter-box" bindtap="toServiceIndexFun">立即问诊</view>
    </view>
  </view>
</view>