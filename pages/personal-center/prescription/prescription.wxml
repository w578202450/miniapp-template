
<block wx:if="{{noData}}">
  <tmcTipsView icon="/images/public/public_no_prescription.png" title="暂无处方" message="您暂无处方信息，去咨询医生吧～" buttonTitle="咨询医生" bind:buttonOption="noDataOption"></tmcTipsView>
</block>
<block wx:elif="{{noNetwork}}">
  <tmcTipsView icon="/images/public/public_no_internet.png" title="网络连接异常" message="网络不太好，刷新一下吧～" buttonTitle="刷新" bind:buttonOption="noNetworkOption"></tmcTipsView>
</block>
<block wx:else>
  <view class="tmc-container">
    <navigator wx:key="keyID" wx:for="{{list}}" wx:for-item="item" wx:for-index="index" 
    url="/pages/personal-center/prescription-details/prescription-details?rpID={{item.keyID}}&index=0&isPreviewRp=1">
      <view class="cell">
        <view>
          <text class="content">{{item.diagnosis}}</text>
        </view>
        <text class="date">开放日期：{{item.modifyTime}}</text>
      </view>
    </navigator>
  </view>
</block>

<text>{{error}}</text>
