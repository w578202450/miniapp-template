.medical-box {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.button-hollowOut {
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left: 30rpx;
  padding-right: 30rpx;
  margin-bottom: 32rpx;
  margin-right: 8rpx;
}

.button-hollowOut text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 28rpx;
}

.selected-view {
  background-color: rgba(67, 139, 239, 1);
  border-radius: 28rpx;
  border: 1rpx solid rgba(67, 139, 239, 1);
}

.selected-view text {
  color: rgba(255, 255, 255, 1);
}

.normal-view {
  border-radius: 28rpx;
  border: 1rpx solid rgba(218, 218, 218, 1);
}

.normal-view text{
  color:rgba(153,153,153,1);
}
